{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"cli": {
		"packageManager": "npm"
	},
	"newProjectRoot": "projects",
	"projects": {
		"travel-mfe": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/travel-mfe",
			"sourceRoot": "projects/travel-mfe/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-architects/native-federation:build",
					"options": {},
					"configurations": {
						"production": {
							"target": "travel-mfe:esbuild:production"
						},
						"development": {
							"target": "travel-mfe:esbuild:development",
							"dev": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-architects/native-federation:build",
					"options": {
						"target": "travel-mfe:serve-original:development",
						"rebuildDelay": 500,
						"dev": true,
						"cacheExternalArtifacts": false,
						"port": 0
					}
				},
				"test": {
					"builder": "@angular/build:unit-test"
				},
				"esbuild": {
					"builder": "@angular/build:application",
					"options": {
						"browser": "projects/travel-mfe/src/main.ts",
						"tsConfig": "projects/travel-mfe/tsconfig.app.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/travel-mfe/public"
							}
						],
						"styles": [
							"projects/travel-mfe/src/styles.css"
						],
						"polyfills": [
							"es-module-shims"
						]
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve-original": {
					"builder": "@angular/build:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "travel-mfe:esbuild:production"
						},
						"development": {
							"buildTarget": "travel-mfe:esbuild:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4200
					}
				}
			}
		},
